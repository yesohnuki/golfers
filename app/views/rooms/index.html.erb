<% @another_entries.each do |entry| %>
  <%= link_to room_path(entry.room) do %>
    <div class="card">
      <div class="card-body">
        <div>
          <div class="d-flex">
            <div class="chat-avatar">
              <object>
                <%= link_to user_path(entry.user) do %>
                  <%= image_tag entry.user.get_profile_image(80, 80) %>
                <% end %>
              </object>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
<% end %>


